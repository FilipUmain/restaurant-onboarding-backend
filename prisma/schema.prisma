
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Restaurant {
  id String @id 
  name String 
  rating Int
  filter_ids String[]
  image_url String
  delivery_time_minutes Int
  price_range_id String
}

model Filter {
  id String @id
  name String 
  image_url String
}
